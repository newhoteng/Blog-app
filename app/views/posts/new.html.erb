<div>
  <%= form_with model: @post, url: user_posts_path(@user) do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title, autofocus: true, required: true %>
    <%= form.label :text %>
    <%= form.text_field :text, autofocus: true, required: true %>
    <%= form.submit "Post" %>
  <% end %>
</div>
